{
  "name": "championgg-power-parser",
  "version": "1.0.0",
  "description": "Parses champion.gg to get the best champions to ban.",
  "main": "main.prod.js",
  "repository": "https://github.com/bencooper222/league-power-parser",
  "author": "Benjamin Cooper \\\\ benc.io",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "parcel-bundler": "^1.9.4",
    "prettier": "^1.13.7"
  },
  "scripts": {
    "lint": "prettier --write parsing/src/* *.md",
    "cgg:bundle": "parcel build parsing/src/championgg.js --target browser --out-dir ./parsing/dist --no-source-maps --out-file championgg.prod.js --experimental-scope-hoisting",
    "cgg:listen": "parcel parsing/src/championgg.js --target browser --out-file championgg.dev.js --out-dir ./parsing/dev --no-source-maps",
    "opgg:bundle": "parcel build parsing/src/opgg.js --target browser --out-dir ./parsing/dist --no-source-maps --out-file opgg.prod.js --experimental-scope-hoisting",
    "opgg:listen": "parcel parsing/src/opgg.js --target browser --out-file opgg.dev.js --out-dir ./parsing/dev --no-source-maps",
    "ugg:bundle": "parcel build parsing/src/ugg.js --target browser --out-dir ./parsing/dist --no-source-maps --out-file ugg.prod.js --experimental-scope-hoisting",
    "ugg:listen": "parcel parsing/src/ugg.js --target browser --out-file ugg.dev.js --out-dir ./parsing/dev --no-source-maps",
    "all:bundle": "yarn cgg:bundle & yarn opgg:bundle & yarn ugg:bundle",
    "all:listen": "yarn cgg:listen & yarn opgg:listen & yarn ugg:listen"
  },
  "dependencies": {
    "lodash.reduce": "^4.6.0"
  }
}
